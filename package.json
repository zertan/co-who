{
  "name": "co-who",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "shadow:watch": "shadow-cljs watch main",
    "shadow:release": "shadow-cljs release main",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss src/css/index.css -o ./out/css/main.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch NODE_OPTIONS=--max-old-space-size=8000 postcss src/css/index.css -o ./out/css/main.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss src/css/index.css -o ./out/main.css --verbose",
    "dev": "run-p -l *:watch",
    "ceramic:local": "CERAMIC_ENABLE_EXPERIMENTAL_COMPOSE_DB='true' ceramic daemon --network inmemory",
    "ceramic:deploy": "composedb composite:deploy ./src/main/composedb/my-composite.json --ceramic-url=http://localhost:7007",
    "ceramic:graphql": "composedb graphql:server --ceramic-url=http://localhost:7007 --graphiql ./src/main/composedb/runtime-composite.json --port=5005",
    "release": "run-s *:release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "postcss": "^8.4.35",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@ceramicnetwork/http-client": "^5.2.0",
    "@ceramicnetwork/streamid": "^4.1.0",
    "@composedb/client": "^0.6.0",
    "@didtools/pkh-ethereum": "^0.5.0",
    "@interactjs/actions": "^1.10.26",
    "@interactjs/interact": "^1.10.26",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@scure/base": "^1.1.5",
    "@scure/bip32": "^1.3.3",
    "@scure/bip39": "^1.2.2",
    "abitype": "^1.0.1",
    "blockies-ts": "^1.0.0",
    "cid": "github:multiformats/cid",
    "co-blue": "link:../co-blue",
    "did-session": "^2.1.2",
    "flowbite": "^2.3.0",
    "flowbite-datepicker": "^1.2.6",
    "isows": "^1.0.3",
    "mr-who": "link:../mr-who",
    "multiformats": "^13.1.0",
    "navigo": "^8.11.1",
    "sha3": "link:@noble/hashes/sha3",
    "varint": "^6.0.0",
    "viem": "^2.7.14"
  }
}
