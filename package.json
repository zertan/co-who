    "cross-env": "^7.0.3",
    "postcss": "^8.4.35",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4",
    "vite-plugin-minify": "^1.5.2"
  },
  "dependencies": {
